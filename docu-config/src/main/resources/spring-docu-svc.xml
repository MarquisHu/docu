<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-autowire="byName">

    <!-- 用户模块service start -->
	<bean id="userService" class="com.docu.user.service.impl.UserServiceImpl"></bean>
	
	<!-- 账户模块service start -->
	<bean id="accountService" class="com.docu.account.service.impl.AccountServiceImpl"></bean>
	<bean id="detailService" class="com.docu.account.service.impl.AccountDetailServiceImpl"></bean>
	
	<!-- 活动模块service start -->
	<bean id="activityService" class="com.docu.activity.service.impl.ActivityServiceImpl"></bean>
</beans>